  <script src="https://unpkg.com/tippy.js@6"></script>
  <script>
    tippy('#github-btn', {
      content: 'View on GitHub',
      placement: 'bottom'
    });
    
    tippy('#docs-btn', {
      content: 'Getting started with SearQ',
      placement: 'bottom'
    });
    
    tippy('#export-tool-btn', {
      content: 'Try the awesome export Tool',
      placement: 'bottom'
    });
    
    tippy('#export-btn', {
      content: 'Export data as CSV (max 1000 items per query)',
      placement: 'bottom'
    });

    tippy('#stats-btn', {
      content: 'See some stats about data',
      placement: 'bottom'
    });

    tippy('#flows-tool-btn', {
      content: 'Create your own flows',
      placement: 'bottom'
    });

    tippy('#flows-btn', {
      content: 'Create your own searq endpoint',
      placement: 'bottom'
    });
  </script>
</html>
